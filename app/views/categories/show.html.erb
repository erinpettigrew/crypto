<% title @category.name %>
<% description '' %>
<% keywords '' %>


<%= render partial: 'meta', locals: { category: @category } %>
<!-- <%= render 'products/search' %> -->
<div class="page">
<%= render partial: 'header_show', locals: { category: @category } %>


<div class="sortable">
  <%= render partial: 'products/product_row', collection: @products, as: :product %>
</div>
</div>

