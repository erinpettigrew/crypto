<% nofollow %>
<% noindex %>
<div class="form-titles"><h1>Edit your review</h1></div>
<div class = "row product-panel-form">
  <div class = "col-xs-4 col-sm-12">
    <%= image_tag @product.image_url %>
  </div>
  <div class = "col-xs-8 col-sm-12">
    <h3><%= @product.product_brand %></h3>
    <p><%= @product.name %></p>

  </div>
</div>

<%= render 'form' %>

<%= link_to 'Back', product_path(@product) %>
