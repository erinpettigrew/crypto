<% nofollow %>
<% noindex %>

<div class="title">You</div>
<h1>Just one more thing!</h1>

<div class="large-avatar"><%= image_tag(@user.avatar.try(:image)) %></div>

<%= form_for @user do |f| %>
  <div class="form-group">
    <%= f.text_field :user_name, value: "", class: "form-control onboarding" %>
  </div>
  <%= f.submit "Done!", class: "btn btn-lg btn-primary" %>
<% end %>
